{
  "name": "230-as-physiotheraphy-website",
  "version": "1.0.0",
  "description": "Static site built with Eleventy and Laravel Mix for 230 AS Physiotheraphy",
  "main": "index.js",
  "scripts": {
    "watch": "ELEVENTY_ENV=development npx @11ty/eleventy --config=eleventy.config.js --serve & webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "debug": "DEBUG=Eleventy* ELEVENTY_ENV=development npx @11ty/eleventy --config=eleventy.config.js --serve & webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "NODE_ENV=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && npx @11ty/eleventy --config=eleventy.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:granttransition/230-AS-Physiotheraphy.git"
  },
  "author": "Grant Smith",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-navigation": "^0.1.6",
    "clean-css": "^4.2.3",
    "html-minifier": "^4.0.0",
    "laravel-mix": "^5.0.6",
    "laravel-mix-purgecss": "^4.2.0",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.27.0",
    "sass-loader": "^8.0.0",
    "terser": "^5.3.4",
    "vue-template-compiler": "^2.6.12"
  },
  "dependencies": {
    "@quasibit/eleventy-plugin-sitemap": "^2.0.2"
  }
}
