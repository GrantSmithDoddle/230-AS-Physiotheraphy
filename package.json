    "@babel/preset-env": "^7.12.1",
    "autoprefixer": "^10.0.1",
    "cssnano": "^4.1.10",
    "get-google-fonts": "^1.2.2",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-better-rollup": "^4.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-plumber": "^1.2.1",
